{
  "name": "route-path-tester",
  "displayName": "routePathTester",
  "publisher": "mxydl2009",
  "description": "routePathTester for testing the matched result between react route path and pathname of url",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mxydl2009/route-path-tester.git"
  },
